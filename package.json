{
  "name": "mit-fullstack-banking-application",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://0.0.0.0:3001",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongodb": "^4.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.2.1",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "concurrently \"nodemon index.js\" \"BROWSER=none react-scripts start\"",
    "docker:build": "docker build -t mit-fullstack-banking-application --progress=plain .",
    "docker:start": "docker compose up --force-recreate --build --detach",
    "docker:log": "docker compose logs -f",
    "docker:stop": "docker compose down",
    "docker:remove": "docker compose rm -svf",
    "postdocker:start": "npm run docker:log",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "eslint": "^8.11.0",
    "eslint-plugin-react": "^7.29.4",
    "nodemon": "^2.0.16"
  }
}
